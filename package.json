{
  "name": "gp-to-services",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": "6.x.x"
  },
  "scripts": {
    "start": "node app.js",
    "lint": "eslint .",
    "lint-dev": "esw --watch .",
    "test": "mocha --recursive test",
    "test-dev": "npm run test -- --watch",
    "coverage-report": "istanbul cover _mocha -- --recursive test",
