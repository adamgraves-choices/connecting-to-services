    "coveralls": "cat ./coverage/lcov.info | coveralls"
  },
  "pre-commit": [
    "lint",
    "test",
    "check-coverage"
  ],
  "dependencies": {
    "body-parser": "^1.13.3",
    "compression": "^1.5.2",
    "cookie-parser": "^1.3.3",
    "dotenv": "^2.0.0",
    "express": "^4.13.3",
    "method-override": "^2.3.0",
    "morgan": "^1.6.1",
    "nunjucks": "^2.4.2",
    "require-environment-variables": "^1.1.2"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "coveralls": "^2.11.9",
    "eslint": "^2.11.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.8.1",
    "eslint-plugin-jsx-a11y": "^1.3.0",
    "eslint-plugin-react": "^5.1.1",
    "eslint-watch": "^2.1.11",
    "grunt": "^1.0.1",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-develop": "^0.4.0",
    "istanbul": "^0.4.3",
    "load-grunt-tasks": "^3.2.0",
    "mocha": "^2.5.3",
    "nock": "^8.0.0",
    "node-mocks-http": "^1.5.2",
    "pre-commit": "^1.1.3",
    "request": "^2.60.0",
    "time-grunt": "^1.2.1"
